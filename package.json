{
  "name": "@microeinhundert/eslint-config-react",
  "version": "1.0.8",
  "description": "ESLint config for react projects",
  "main": "src/index.js",
  "author": "Leon Seipp <l.seipp@microeinhundert.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
